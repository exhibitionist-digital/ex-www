{
  "tasks": {
    "run": "deno run -A server.tsx",
    "dev": "deno run -A --no-check --watch=content,src/components,src/app.tsx,src/hooks,server.tsx --reload --location=http://localhost:8000 ./dev.ts",
    "test": "deno test --allow-all",
    "build": "deno run -A ./build.ts",
    "start": "ULTRA_MODE=production deno run -A --no-remote ./server.js"
  },
  "compilerOptions": {
    "jsx": "react-jsxdev",
    "jsxImportSource": "react"
  },
  "importMap": "./importMap.json"
}
